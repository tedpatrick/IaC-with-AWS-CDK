{
	"name": "web-backend",
	"version": "0.1.0",
	"license": "MIT",
	"author": "Ted Patrick",
	"description": "AWS CDK stack for web backend with HTTP API Gateway and Lambda using Node.js and hono",
	"scripts": {
		"bootstrap": "cdk bootstrap",
		"build": "tsc --noemit",
		"cdk": "cdk",
		"deploy": "cdk deploy --require-approval never --outputs-file outputs.json",
		"destroy": "cdk destroy --force"
	},
	"dependencies": {
		"@types/node": "22.15.24",
		"aws-cdk": "2.1017.0",
		"aws-cdk-lib": "2.199.0",
		"constructs": "10.4.2",
		"source-map-support": "0.5.21",
		"ts-node": "10.9.2",
		"typescript": "5.8.3"
	}
}